public subroutine of_check_job_status ();DataStore lds

lds = Create DataStore

lds.DataObject = "d_check_job_status"
lds.SetTransObject(SQLCA)

lds.Retrieve(SQLCA.database+"DailyBackup")

if(lds.RowCount() >= 1)Then

	tab_setup.tabpage_app.tab_1.tabpage_13.cb_5.enabled				= False
	
	if( lds.GetItemNumber(1 , "enabled") = 0 )Then
		tab_setup.tabpage_app.tab_1.tabpage_13.pb_disable.enabled	= False
		tab_setup.tabpage_app.tab_1.tabpage_13.pb_enabled.enabled	= True
	else
		tab_setup.tabpage_app.tab_1.tabpage_13.pb_disable.enabled	= True
		tab_setup.tabpage_app.tab_1.tabpage_13.pb_enabled.enabled	= False
	end if
	
else

	tab_setup.tabpage_app.tab_1.tabpage_13.cb_5.enabled 			= True
	tab_setup.tabpage_app.tab_1.tabpage_13.pb_disable.enabled	= False
	tab_setup.tabpage_app.tab_1.tabpage_13.pb_enabled.enabled	= False
		
end if

destroy lds
end subroutine

public subroutine of_set_sales_ini ();String ls_inifile

ls_inifile = gnv_app.of_getappinifile( )

SetProfileString( ls_inifile , "DataBase" ,  "SalesInvoice"  , tab_setup.tabpage_app.tab_1.tabpage_1.sle_inv_type.text)
SetProfileString( ls_inifile , "POS" 		,  "InvoiceCopies" , tab_setup.tabpage_app.tab_1.tabpage_1.em_copies.text )
SetProfileString( ls_inifile , "POS" 		,  "posinvnote1"   , tab_setup.tabpage_app.tab_1.tabpage_1.sle_note1.text )
SetProfileString( ls_inifile , "POS" 		,  "posinvnote2"   , tab_setup.tabpage_app.tab_1.tabpage_1.sle_note2.text )

IF tab_setup.tabpage_app.tab_1.tabpage_1.cbx_autoprint.checked THEN
	SetProfileString( ls_inifile , "POS" ,  "AutoPrintInvoice" ,  "1" )
ELSE
	SetProfileString( ls_inifile , "POS" ,  "AutoPrintInvoice" ,  "0" )
END IF

IF  tab_setup.tabpage_app.tab_1.tabpage_1.cbx_autonew.checked  THEN
	SetProfileString(ls_inifile , "POS" ,  "AutoNewInvoice" ,  "1" )
ELSE
	SetProfileString(ls_inifile , "POS" ,  "AutoNewInvoice" ,  "0" )
END IF

IF tab_setup.tabpage_app.tab_1.tabpage_1.cbx_remove_duplicated.checked THEN
	SetProfileString(ls_inifile , "DataBase" ,  "RemoveDuplicatItems" , "Yes" )
ELSE
	SetProfileString(ls_inifile , "DataBase" ,  "RemoveDuplicatItems" , "No" )
END IF

IF tab_setup.tabpage_app.tab_1.tabpage_1.cbx_popup.checked  THEN
	SetProfileString(ls_inifile , "POS" ,  "popupdisplay" ,  "1" )
ELSE
	SetProfileString(ls_inifile , "POS" ,  "popupdisplay" ,  "0" )
END IF

IF tab_setup.tabpage_app.tab_1.tabpage_1.cbx_printer_type.checked THEN
	SetProfileString(ls_inifile , "POS" ,  "INVOICE" ,  "1" )
ELSE
	SetProfileString(ls_inifile , "POS" ,  "INVOICE" ,  "2" )
END IF

end subroutine

public subroutine of_email_settings ();DataStore  ds
String ls_smtp_auth = "N",ls_cypto

ds = CREATE DataStore
ds.DataObject = "d_email_settings"
ds.SetTransobject( SQLCA )

IF ds.Retrieve( ) <= 0 THEN
	Destroy ds
	Return 
END IF

IF Len(Trim(tab_setup.tabpage_app.tab_1.tabpage_14.sle_password.text)) < 6 THEN
	Messagebox("تنبيه","يجب ان  تكون كلمة السر اكثر من 6 خانات")
	Return 
END IF

IF tab_setup.tabpage_app.tab_1.tabpage_14.cbx_smtpauth.checked THEN
	ls_smtp_auth = "Y"
ELSE
	ls_smtp_auth = "N"
END IF

IF tab_setup.tabpage_app.tab_1.tabpage_14.rb_none.checked THEN ls_cypto = "N" 
IF tab_setup.tabpage_app.tab_1.tabpage_14.rb_ssl.checked THEN ls_cypto 	= "S"
IF tab_setup.tabpage_app.tab_1.tabpage_14.rb_tls.checked THEN ls_cypto 	= "T"

ds.SetItem(1 , "string_val" 	, tab_setup.tabpage_app.tab_1.tabpage_14.sle_server.text )
ds.SetItem(1 , "int_val" 		, Long(tab_setup.tabpage_app.tab_1.tabpage_14.sle_port.text) )
ds.SetItem(1 , "char_val" 		, ls_smtp_auth )
ds.SetItem(2 , "string_val"	, tab_setup.tabpage_app.tab_1.tabpage_14.sle_userid.text )
ds.SetItem(2 , "char_val"		, ls_cypto )
ds.SetItem(3 , "string_val"	, gnv_services.of_encrypt_password(tab_setup.tabpage_app.tab_1.tabpage_14.sle_password.text))

ds.update()

Commit;

Destroy ds
end subroutine

on w_app_setup.create
int iCurrent
call super::create
this.p_1=create p_1
this.cb_cancel=create cb_cancel
this.cb_ok=create cb_ok
this.tab_setup=create tab_setup
iCurrent=UpperBound(this.Control)
this.Control[iCurrent+1]=this.p_1
this.Control[iCurrent+2]=this.cb_cancel
this.Control[iCurrent+3]=this.cb_ok
this.Control[iCurrent+4]=this.tab_setup
end on

on w_app_setup.destroy
call super::destroy
destroy(this.p_1)
destroy(this.cb_cancel)
destroy(this.cb_ok)
destroy(this.tab_setup)
end on

