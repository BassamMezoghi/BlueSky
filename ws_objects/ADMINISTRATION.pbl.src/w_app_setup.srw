	tab_setup.tabpage_app.tab_1.tabpage_1.cbx_popup.checked = True
ELSE
	tab_setup.tabpage_app.tab_1.tabpage_1.cbx_popup.checked = False
END IF

IF ProfileString(ls_inifile , "POS" ,  "INVOICE" , "2" ) = "1" THEN
	tab_setup.tabpage_app.tab_1.tabpage_1.cbx_printer_type.checked = True
ELSE
	tab_setup.tabpage_app.tab_1.tabpage_1.cbx_printer_type.checked = False
