 retrieve="SELECT  CUSTOMER.CUSTOMER_ID ,
           CUSTOMER.CUSTOMER_NM ,
           CUSTOMER.INITIAL_BALANCE ,
           CUSTOMER.INITIAL_DT ,
           CUSTOMER.BALANCE ,
           CUSTOMER.BALANCE_DT ,
           CUSTOMER.SHOP ,
           CUSTOMER.SALEMAN ,
           CUSTOMER.PHONE ,
           CUSTOMER.CEIL ,
           CUSTOMER.PHONE2 ,
           CUSTOMER.ACTIVE_FG ,
           CUSTOMER.LAST_MODIFIED_BY ,
           CUSTOMER.LAST_MODIFIED_DT ,
           CUSTOMER.rank ,           
           CUSTOMER.address ,
           CUSTOMER.ceil2 ,
           CUSTOMER.kind ,
           CUSTOMER.SHOP_KIND ,
           CUSTOMER.SHOP_ID ,
           CUSTOMER.OWNER_ADDRESS ,
           CUSTOMER.OWNER_ID_NO ,
           CUSTOMER.OWNER_PHONE ,
           CUSTOMER.SALEMAN_ADDRESS ,
           CUSTOMER.SALEMAN_PHONE ,
           CUSTOMER.SALEMAN_ID_NO ,
           CUSTOMER.NOTE ,
           CUSTOMER.CURRENCY_ID ,
           CUSTOMER.INITIAL_ACCOUNT_IN_OTHER     
        FROM CUSTOMER      
        WHERE ( CUSTOMER.ACTIVE_FG = 'Y' ) and          
              ( CUSTOMER.kind      = 'M' )   and
				  ( CUSTOMER.CUSTOMER_ID = :id )" update="CUSTOMER" updatewhere=0 updatekeyinplace=no arguments=(("id", number)) )
